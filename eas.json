{
  "cli": {
    "version": ">= 3.13.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON_BASE64": "$(GOOGLE_SERVICES_JSON_BASE64)"
      },
      "hooks": {
        "prebuild": "node android/scripts/decode_google_services.js"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {}
  }
}